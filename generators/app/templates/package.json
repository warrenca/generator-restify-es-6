{
  "name": "<%=appname%>",
  "version": "0.0.1",
  "description": "<%=app_description%>",
  "main": "app.js",
  "dependencies": {
    "async": "^2.0.0-rc.1",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "bunyan": "^1.8.1",
    "consign": "^0.1.2",
    "dotenv": "^2.0.0",
    "moment": "^2.12.0",
    "node-service-locator": "0.0.2",
    "pm2": "^1.1.3",
    "restify": "^4.1.0",
    "restify-errors": "^4.2.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint-config-google": "^0.4.0",
    "grunt": "~0.4.5",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-eslint": "^18.0.0",
    "grunt-mocha-test": "^0.12.7",
    "grunt-notify": "^0.4.5",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "scripts": {
    "start": "node app.js",
    "local": "node_modules/.bin/pm2 startOrRestart ecosystem.json5 && node_modules/.bin/pm2 logs <%=appname%> --lines=20",
    "test": "NODE_ENV=test mocha test/**/*.js",
    "stop": "node_modules/.bin/pm2 kill && node_modules/.bin/pm2 flush && node_modules/.bin/pm2 list",
    "restart-local": "npm stop && npm run local"
  },
  "repository": {
    "type": "git",
    "url": "<%=project_url%>"
  }
}
