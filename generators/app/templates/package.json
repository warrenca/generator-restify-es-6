    "grunt-env": "^0.4.4",
    "grunt-eslint": "^20.0.0",
    "grunt-mocha-test": "^0.13.2",
    "grunt-notify": "^0.4.5",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "node-service-locator": "^0.0.2",
    "pm2": "^2.5.0",
    "restify": "^4.3.0",
